<h1>Users#new</h1>

<%=form_for @user do |f|%>
<%=f.label :name%>
<%=f.text_field :name%>
<%=f.label :age%>
<%=f.number_field :age%>
<%=f.label :city%>
<%=f.text_field :city%>
<%=f.label :gender%>
<%=f.text_field :gender%>
<%=f.label :email%>
<%=f.email_field :email%>
<%=f.label :bio%>
<%=f.text_field :bio%>
<%=f.label :password%>
<%=f.password_field :password%>
<br>
<br>
<h1><%=f.submit "Sign up"%></h1>


<%end%>


