<h1>Users#edit</h1>


<%=form_for @user do |f|%>
<%=f.label :name%>
<%=f.text_field :name%>
<%=f.label :city%>
<%=f.text_field :city%>
<%=f.label :gender%>
<%=f.text_field :gender%>
<%=f.label :email%>
<%=f.text_field :email%>
<%=f.label :bio%>
<%=f.text_field :bio%>
<br>
<br>
<h1><%=f.submit "Update"%></h1>


<%end%>

